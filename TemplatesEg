#include <iostream>
using namespace std;
template <typename T>
T maxm(T a, T b) {
	return (a > b) ? a : b;
}

template <typename T>
T minm(T a, T b) {
	return (a < b) ? a : b;
}

int main() {
	cout << maxm(5, 6) << endl;
	cout << maxm((double)5, 6.6) << endl;
	cout << maxm<double>(5, 6.6) << endl;
}
//Output:
//6
